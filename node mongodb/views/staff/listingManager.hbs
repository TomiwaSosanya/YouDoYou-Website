            <td>{{this.catagory}}</td>
            <td>{{this.price}}</td>
            <td>{{this.quantity}}</td>
            <td>{{this.variations}}</td>
            <td>{{this.shipping}}</td>
            <td>
                <a class="btn btn-primary btn-sm" href="/staff/listing/{{this.id}}"> Edit</a>
                <a class="btn btn-primary btn-sm" href="/staff/listing/delete/{{this.id}}"
                onclick="return confirm('are you sure you want to delete this record?');">Delete</a>
            </td>
        </tr>
        {{/each}}
