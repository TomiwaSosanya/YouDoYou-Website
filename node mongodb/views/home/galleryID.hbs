<h1 style="top:200px; left: 0px;">Gallery ID</h1>

<div class="gidvancustomisesection">
    <div id="gidvanscustomisebox">
        <p style="padding-left: 10px;">By:{{customname}} likes: {{vanlikes}}</p>
        <img class="vanscustomP1" id="gvansole" src="data:image/png;base64,{{vancustom.sole}}" width="100%"
            height="100%">
        <img class="vanscustomP2" id="gvanmiddle" src="data:image/png;base64,{{vancustom.middle}}" width="100%"
            height="100%">
        <img class="vanscustomP3" id="gvanfrontandback" src="data:image/png;base64,{{vancustom.frontandback}}"
            width="100%" height="100%">
        <img class="vanscustomP4" id="gvanfront" src="data:image/png;base64,{{vancustom.front}}" width="100%"
            height="100%">
        <img class="vanscustomP5" id="gvanlaces" src="data:image/png;base64,{{vancustom.laces}}" width="100%"
            height="100%">
        <img class="vanscustomP6" id="gvantop" src="data:image/png;base64,{{vancustom.tounge}}" width="100%"
            height="100%">
        <img class="vanscustomP7" id="gvantounge" src="data:image/png;base64,{{vancustom.top}}" width="100%"
            height="100%">
        <img class="vanscustomP8" id="gvanstripe" src="data:image/png;base64,{{vancustom.stripe}}" width="100%"
            height="100%">
    </div>

    <a href="/home/add-to-cart/{{item}}" class="btn btn-primary" role="button">Add To Cart</a>
</div>



